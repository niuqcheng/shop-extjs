  "scripts": {
    "dev": "vite",
    "dev:vue-tsc": "vue-tsc --noEmit && cross-env VITE_CJS_IGNORE_WARNING=true vite",
    "dev:https": "cross-env VITE_CJS_IGNORE_WARNING=true vite --config ./vite.config.dev.ts",
    "vue-tsc": "vue-tsc --noEmit",
    "build": "vite build",
    "build:fast": "node scripts/.hidden-check.js && cross-env VITE_CJS_IGNORE_WARNING=true NODE_OPTIONS=--max-old-space-size=8192 vite build",
    "build:report": "cross-env VITE_CJS_IGNORE_WARNING=true NODE_OPTIONS=--max-old-space-size=8192 vite build --config ./vite.config.dev.ts",
    "build:website": "node scripts/.hidden-check.js && cross-env VITE_CJS_IGNORE_WARNING=true NODE_OPTIONS=--max-old-space-size=8192 vite build --config ./vite.config.website.ts && node write.version.js",
    "build:vue-tsc": "vue-tsc --noEmit && cross-env VITE_CJS_IGNORE_WARNING=true vite build",
    "preview": "cross-env VITE_CJS_IGNORE_WARNING=true vite preview",
    "lint:eslint": "eslint . --fix",
    "lint:oxlint": "oxlint",
    "lint:oxlint:fix": "oxlint --fix",
    "lint:prettier": "prettier {src,mock,library,types}/**/*.{html,vue,css,sass,scss,js,ts} --write",
    "lint:stylelint": "stylelint {src,mock,library}/**/*.{vue,css,sass,scss} --fix --cache --cache-location node_modules/.cache/stylelint/",
    "global:install": "pnpm add -g nrm cnpm npm-check-updates pnpm",
    "globle:update": "ncu -g",
    "module:install": "pnpm install",
    "module:update": "ncu -u --reject vite-plugin-mock,@vitejs/plugin-vue-jsx,vue-echarts,chokidar,@opentiny/vue,@logicflow/core,@logicflow/extension && pnpm run module:install",
    "module:reinstall": "rimraf node_modules && pnpm run module:install",
    "git": "start ./git.sh",
    "generate-pwa-assets": "pwa-assets-generator",
    "protect": "node scripts/protection.js",
    "template": "plop",
    "predev": "node scripts/.hidden-check.js",
    "prebuild": "node scripts/.hidden-check.js"
  },
